{
  "manifest_version": 3,
  "name": "Web Decoder",
  "description": "A helper to decode text encodings such as base64 or UNIX timestamps",
  "version": "2.5.2",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}